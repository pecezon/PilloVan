
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}
        
        

model Perro{
  id        Int      @id @default(autoincrement())
  nombre    String
  raza      String
  edad      Int
  peso      Float
  vacunado  Boolean
}